<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="478.0" prefWidth="678.0"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="ru.nonclassicalLogic.lab1.Controller">

    <children>
        <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" GridPane.rowSpan="2" />
        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <children>
                <TextField prefHeight="25.0" prefWidth="70.0" />
                <TextField prefHeight="25.0" prefWidth="70.0" />
                <TextField prefHeight="25.0" prefWidth="70.0" />
                <TextField prefHeight="25.0" prefWidth="70.0" />
                <TextField prefHeight="25.0" prefWidth="70.0" />
                <Button fx:id="btn" onAction="#click" text="Button" />
            </children>
        </HBox>
        <LineChart GridPane.columnIndex="1">
            <xAxis>
                <CategoryAxis side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT" />
            </yAxis>
        </LineChart>
    </children>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
</GridPane>
